<?php

/**
 * @file
 * File provide code for installation easy authcache views module
 *
 */

/**
 * Implementation hook_install()
 */
function easy_authcache_views_install() {
    variable_set('easy_authcache_views', array());
}

/**
 * Implementation hook_uninstall()
 */
function easy_authcache_views_uninstall() {
    variable_del('easy_authcache_views');
}
